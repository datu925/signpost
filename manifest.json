{
  "manifest_version": 2,

  "name": "Breadcrumbs",
  "description": "This extension allows you to assign keyboard shortcuts to links",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Activate shortcuts!"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["application.js"],
    "persistent": false
  }
}