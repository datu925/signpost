{
  "manifest_version": 2,

  "name": "SignPost",
  "description": "This extension allows you to assign keyboard shortcuts to links",
  "version": "1.3",
  "browser_action": {
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["application.js"],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+K"
      }
    }
  }
}